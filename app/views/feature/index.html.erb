<h1>Index</h1>

<table class="table">
<thead>
	<tr>
		<th scope="col">Project</th>
		<th scope="col">Code</th>
		<th scope="col">Feature</th>
		<th scope="col">Description</th>
	</tr>
</thead>
<% @features.each do |feature| %>
	<tr>
		<td><%= feature.project.name %></td>
		<td><%= feature.code %></td>
		<td><%= feature.title %></td>
		<td><%= feature.description %></td>
	</tr>
	<% end %>
</table>